<script type="text/javascript">
    $( document ).ready(function() {
        $('.burger, .navbar-right-overlay').click(function() {
            main_menu_isclose() ? main_menu_open() : main_menu_close()
        });
        $('.uk-navbar-nav > li > ul').prev().click(function(){
            if ($(window).width() >= 1031){
                return true
            }
            else{ $(this).parent().toggleClass('mobile-open'); return false;}})
        $(window).resize(function() {
            if($(window).width() >= 1031) {
                $('#mobile-menu').after($('.button_blue').eq(0));
                main_menu_open();
            }
            else{
                $('#mobile-menu').before($('.button_blue').eq(0));
                main_menu_close()
            }
        }).resize();
    });
    function main_menu_isclose(){
        return $('.burger').attr('uk-icon')=='icon: menu'
    }
    function main_menu_open(){
        $('.burger').attr('uk-icon', 'icon: close');
        $('#mobile-menu').attr('aria-hidden', 'false').removeAttr('hidden');
    }
    function main_menu_close(){
        $('.burger').attr('uk-icon', 'icon: menu');
        $('#mobile-menu').attr('aria-hidden', 'true').attr('hidden', 'hidden');
    }
</script>

