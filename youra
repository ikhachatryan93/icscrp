<script type="text/javascript">
    $(document).ready(function () {
        $('#get-rating-header').on('click', function () {
            $('html, body').animate({
                scrollTop: $("#get-rating-form").offset().top
            }, 1000);
            ga('send', 'event', 'submint', 'getratinghead', 'getratinghead');
        });

        $('#get-funds-header').on('click', function () {
            $('html, body').animate({
                scrollTop: $("#contact-us").offset().top
            }, 1000);
        });

        $('input,textarea').focus(function() {
            $(this).data('placeholder',$(this).attr('placeholder'))
            $(this).attr('placeholder','');
        });
        $('input,textarea').blur(function() {
            $(this).attr('placeholder',$(this).data('placeholder'));
        });

        $('#subs-form-home').on('submit', function (event) {
            event.preventDefault();

            $.post(
                'https://icorating.com/subscribe/',
                {
                    'email': $('#subs-form-home').find('[name="email"]').val(),
                    '_token': "oTP6fgSP0bGMPyATbXwCcb6ejHovglgXGeHOYA1p"
                },
                subscribeSuccessHome
            );
        });

        $('#subs-form-home a').on('click', function (event) {
            event.preventDefault();

            $.post(
                'https://icorating.com/subscribe/',
                {
                    'email': $('#subs-form-home').find('[name="email"]').val(),
                    '_token': "oTP6fgSP0bGMPyATbXwCcb6ejHovglgXGeHOYA1p"
                },
                subscribeSuccessHome
            );
        });

        function subscribeSuccessHome(data) {
            $('.form-subscribe-error').remove();
            $('.form-error').hide();
            $('#subs-form-home').find("input, textarea").attr('style','');

            data = JSON.parse(data);
            if(data.success) {
                $('#subs-form-home').find('input').val('');
                $('#ok-modal').addClass('uk-open').show();

                $.post(
                    'https://icorating.com/subscribesend/',
                    {
                        email: data.data.email,
                        '_token': "oTP6fgSP0bGMPyATbXwCcb6ejHovglgXGeHOYA1p"
                    }
                );
                ga('send', 'event', 'submint', 'subscribeindex', 'subscribeindex'); yaCounter46553805.reachGoal('subscribeindex');
            } else {
                $.each(data.errors, function(name, error) {
                    $('#subs-form-home')
                        .find("[name='"+name+"']")
                        .css({'border-bottom': '1px solid #ff0000'})
                        .parents('.uk-inline')
                        .append('<div class="uk-inline form-error" style="color: #ff0000; font-size: 12px;">'+error+'</div>');
                });
            }
        }
        $('#subs-form').on('submit', function (event) {
            event.preventDefault();

            $.post(
                'https://icorating.com/subscribe/',
                {
                    'email': $('#subs-form').find('[name="email"]').val(),
                    '_token': "oTP6fgSP0bGMPyATbXwCcb6ejHovglgXGeHOYA1p"
                },
                subscribeSuccess
            );
        });

        $('#subs-form a').on('click', function (event) {
            event.preventDefault();

            $.post(
                'https://icorating.com/subscribe/',
                {
                    'email': $('#subs-form').find('[name="email"]').val(),
                    '_token': "oTP6fgSP0bGMPyATbXwCcb6ejHovglgXGeHOYA1p"
                },
                subscribeSuccess
            );
        });

        function subscribeSuccess(data) {
            $('.form-subscribe-error').remove();
            $('#subs-form').find("input, textarea").attr('style','');

            data = JSON.parse(data);
            if(data.success) {

                //$('#subs-form').removeClass('uk-open').hide();
                $('#subs-form').find("input[type=text], textarea").val("");
                $('#ok-modal').addClass('uk-open').show();

                $.post(
                    'https://icorating.com/subscribesend/',
                    {
                        email: data.data.email,
                        '_token': "oTP6fgSP0bGMPyATbXwCcb6ejHovglgXGeHOYA1p"
                    }
                );
                ga('send', 'event', 'submint', 'subscribefoter', 'subscribefoter'); yaCounter46553805.reachGoal('subscribefoter');
            } else {
                $.each(data.errors, function(name, error) {
                    $('#subs-form')
                        .find("[name='"+name+"']")
                        .css({'border-bottom': '1px solid #ff0000'})
                        .parents('.uk-inline')
                        .append('<div class="uk-inline form-error" style="color: #ff0000; font-size: 12px;">'+error+'</div>');
                });
            }
        }

        $('#get-rating').on('submit', function (event) {
            event.preventDefault();

            $.post(
                'https://icorating.com/getrating/',
                $('#get-rating').serialize(),
                getRatingSuccess
            );

            ga('send', 'event', 'submint', 'getratinghead', 'getratinghead'); yaCounter46553805.reachGoal('getratinghead');
        });

        $('#contact-us').on('submit', function (event) {
            event.preventDefault();

            $.post(
                'https://icorating.com/contactus/',
                $('#contact-us').serialize(),
                contactUsSuccess
            );
        });

        function contactUsSuccess(data) {
            $('.form-error').remove();
            $('#button-modal').find("input, textarea").attr('style','');
            $('#contact-us').find("input, textarea").attr('style','');

            data = JSON.parse(data);
            if(data.success) {
                $('html, body').animate({
                    scrollTop: $("#contact-us").offset().top
                }, 1000);

                $('#button-modal').removeClass('uk-open').hide();
                $('.thank-you').show();
                $('#button-modal, #contact-us').find("input[type=text], textarea").val("");

                $.post(
                    'https://icorating.com/contactussend/',
                    {
                        email: data.data.email,
                        person: data.data.person,
                        phone: data.data.phone,
                        code: data.data.code,
                        '_token': "oTP6fgSP0bGMPyATbXwCcb6ejHovglgXGeHOYA1p"
                    }
                );

            } else {
                $.each(data.errors, function(name, error) {
                    $('#button-modal, #contact-us')
                        .find("[name='"+name+"']")
                        .css({'border-bottom': '1px solid #ff0000'})
                        .parents('.uk-inline')
                        .append('<div class="uk-inline form-error" style="color: #ff0000; font-size: 12px;">'+error+'</div>');
                });
            }
        }

        function getRatingSuccess(data) {
            $('.form-error').remove();
            $('#button-modal').find("input, textarea").attr('style','');
            $('#get-rating-form').find("input, textarea").attr('style','');

            data = JSON.parse(data);
            if(data.success) {
                ga('send', 'event', 'submint', 'send', 'send');
                $('html, body').animate({
                    scrollTop: $("#get-rating-form").offset().top
                }, 1000);

                $('#button-modal').removeClass('uk-open').hide();
                $('.thank-you_form').show();
                $('#button-modal, #get-rating-form').find("input[type=text], textarea").val("");

                $.post(
                    'https://icorating.com/getratingsend/',
                    {
                        email: data.data.email,
                        enquiry: data.data.enquiry,
                        name: data.data.name,
                        person: data.data.person,
                        phone: data.data.phone,
                        skype_telegram: data.data.skype_telegram,
                        website: data.data.website,
                        '_token': "oTP6fgSP0bGMPyATbXwCcb6ejHovglgXGeHOYA1p"
                    }
                );

            } else {
                $.each(data.errors, function(name, error) {
                    $('#button-modal, #get-rating-form')
                        .find("[name='"+name+"']")
                        .css({'border-bottom': '1px solid #ff0000'})
                        .parents('.uk-inline')
                        .append('<div class="uk-inline form-error" style="color: #ff0000; font-size: 12px;">'+error+'</div>');
                });
            }
        }

    });
</script>
