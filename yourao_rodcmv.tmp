            else{
                $('#mobile-menu').before($('.button_blue').eq(0));
                main_menu_close()
            }
        }).resize();
    });
    function main_menu_isclose(){
        return $('.burger').attr('uk-icon')=='icon: menu'
    }
    function main_menu_open(){
        $('.burger').attr('uk-icon', 'icon: close');
        $('#mobile-menu').attr('aria-hidden', 'false').removeAttr('hidden');
    }
    function main_menu_close(){
        $('.burger').attr('uk-icon', 'icon: menu');
        $('#mobile-menu').attr('aria-hidden', 'true').attr('hidden', 'hidden');
    }
</script>

